{
  "name": "2do2",
  "version": "1.0.0",
  "description": "A powerful CLI tool for managing your Todoist items from the command line",
  "main": "dist/index.js",
  "bin": {
    "2do2": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build",
    "clean": "rm -rf dist",
    "postinstall": "npm run build",
    "link-global": "npm link",
    "unlink-global": "npm unlink -g 2do2"
  },
  "keywords": [
    "todoist",
    "cli",
    "task",
    "management",
    "productivity"
  ],
  "author": "Chris",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "inquirer": "^9.2.0",
    "conf": "^12.0.0",
    "nanoid": "^5.0.0",
    "natural": "^6.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@types/inquirer": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "module"
}
